{
  "name": "electron-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build && npx electron-packager . electron-vue --out=out/build --ignore=src --overwrite",
    "start": "concurrently \"vue-cli-service serve\" \"npx electron .\""
  },
  "main": "electron/main.js",
  "dependencies": {
    "@mdi/font": "5.9.55",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "electron-is-dev": "^2.0.0",
    "pinia": "^2.0.13",
    "roboto-fontface": "*",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.13",
    "vue-router": "^4.0.14",
    "vuetify": "npm:@vuetify/nightly@next",
    "webfontloader": "^1.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "concurrently": "^7.0.0",
    "electron": "^17.1.0",
    "electron-packager": "^15.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.38.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vuetify-loader": "^2.0.0-alpha.0",
    "wait-on": "^6.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
